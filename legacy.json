{
  "packageRules": [
    {
      "allowedVersions": "<3",
      "datasources": ["docker"],
      "packageNames": ["circleci/python", "python"],
      "versioning": "regex:^(?<major>\\d+)\\.(?<minor>\\d+)\\.(?<patch>\\d+)(?<prerelease>(a|b|rc)\\d+)?(-(?<compatibility>.*))?$"
    },
    {
      "packageNames": ["memcached"],
      "datasources": ["docker"],
      "allowedVersions": "<1.5"
    },
    {
      "packageNames": ["mysql"],
      "datasources": ["docker"],
      "allowedVersions": "<5.6"
    },
    {
      "packageNames": ["rabbitmq"],
      "datasources": ["docker"],
      "allowedVersions": "<3.7"
    },
    {
      "packageNames": ["redis"],
      "datasources": ["docker"],
      "allowedVersions": "<3.3"
    }
  ],
  "html": {
    "enabled": false
  },
  "npm": {
    "enabled": false
  },
  "python": {
    "enabled": false
  }
}
