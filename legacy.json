{
  "docker": {
    "packageRules": [
      {
        "allowedVersions": "<3",
        "packageNames": ["circleci/python", "python"],
        "versionScheme": "regex:^(?<major>\\d+)\\.(?<minor>\\d+)\\.(?<patch>\\d+)(?<prerelease>(a|b|rc)\\d+)?(-(?<compatibility>.*))?$"
      },
      {
        "packageNames": ["memcached"],
        "allowedVersions": "<1.5"
      },
      {
        "packageNames": ["mysql"],
        "allowedVersions": "<5.6"
      },
      {
        "packageNames": ["rabbitmq"],
        "allowedVersions": "<3.7"
      },
      {
        "packageNames": ["redis"],
        "allowedVersions": "<3.3"
      }
    ]
  },
  "html": {
    "enabled": false
  },
  "npm": {
    "enabled": false
  },
  "python": {
    "enabled": false
  }
}
